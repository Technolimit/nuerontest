naf.cpp: ../naf.mod $(NOCMODL)
	@printf " -> $(C_GREEN)NMODL$(C_RESET) $<\\n"
	(cd "..";  MODLUNIT=$(NRNUNITS) $(NOCMODL) "naf.mod"  -o "/home/ethan/nuerontest/x86_64" )

./naf.o: naf.cpp
	@printf " -> $(C_GREEN)Compiling$(C_RESET) /home/ethan/nuerontest/x86_64/$<\\n"
	$(CXXCOMPILE) -I".." $(INCLUDES) -fPIC -c "/home/ethan/nuerontest/x86_64/$<" -o $@

